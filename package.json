    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all --parallel dev:client:build dev:server:build dev:server:start",
    "prod": "npm-run-all clean prod:client:build prod:server:build prod:server:start",
    "dev:client:build": "webpack-dev-server --port 9000 --inline  --hot --config webpack/webpack.config.client.dev.babel.js",
    "dev:server:build": "webpack -d --watch --config ./webpack/webpack.config.server.babel.js --colors --display-error-details",
    "dev:server:start": " nodemon ./server/start --watch ./build/server",
    "clean": "rimraf build && mkdirp ./build && mkdirp ./build/static",
    "prod:client:build": "webpack -p --config webpack/webpack.config.client.prod.babel.js",
    "prod:server:build": "webpack -p --config ./webpack/webpack.config.server.babel.js --colors --display-error-details",
    "prod:server:start": " node ./server/start",
    "analyse-bundle": "NODE_ENV=analyse webpack  -p --config webpack/webpack.config.client.prod.babel.js"
  },
