{
  "parser": "babel-eslint",
  "env": {
   "browser": true,
   "node": true
 },
 "plugins": ["react", "import"],
 "extends": "airbnb",
  "rules": {
     "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-console": 0,
      "react/prop-types": 0,
      "prefer-destructuring": 0,
      "no-shadow": 0
  }
}
